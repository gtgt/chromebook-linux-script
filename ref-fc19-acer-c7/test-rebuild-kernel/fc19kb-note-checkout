#!/bin/bash -x
n=353890
while true
do
    n=$(($n-1))
    if [ $n -le 1 ]; then 
        echo "  escape pushing at ~$n"
        break
    fi
    echo git push origin2 master~$n
    #git push origin2 master~$n
    if [ $? -eq 0 ]; then 
        echo "  ok pushed ~$n"
    else
        echo "  failed pushing ~$n"
        break
    fi
    sleep 10

done


[kernel-next]$ git push origin2 8d38ead:refs/heads/master
  49.92 MiB,  18200 objects



